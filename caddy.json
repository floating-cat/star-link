{
  "apps": {
    "http": {
      "servers": {
        "srv0": {
          "automatic_https": {
            "disable_redirects": true
          },
          "listen": [
            ":1101"
          ],
          "logs": {
            "logger_names": {
              "127.0.0.1:1101": "log1",
              "localhost:1101": "log1"
            }
          },
          "routes": [
            {
              "handle": [
                {
                  "handler": "subroute",
                  "routes": [
                    {
                      "handle": [
                        {
                          "handler": "reverse_proxy",
                          "upstreams": [
                            {
                              "dial": ":1200"
                            }
                          ]
                        }
                      ],
                      "match": [
                        {
                          "path": [
                            "/+wck9WA8AWsx/C8k6PibIQ=="
                          ]
                        }
                      ]
                    }
                  ]
                }
              ],
              "match": [
                {
                  "host": [
                    "127.0.0.1",
                    "localhost"
                  ]
                }
              ],
              "terminal": true
            }
          ],
          "tls_connection_policies": [
            {
              "match": {
                "sni": [
                  "127.0.0.1",
                  "localhost"
                ]
              }
            },
            {}
          ]
        },
        "srv1": {
          "automatic_https": {
            "disable": true
          },
          "listen": [
            ":1100"
          ],
          "logs": {
            "logger_names": {
              "127.0.0.1:1100": "log0",
              "localhost:1100": "log0"
            }
          },
          "routes": [
            {
              "handle": [
                {
                  "handler": "subroute",
                  "routes": [
                    {
                      "handle": [
                        {
                          "handler": "reverse_proxy",
                          "upstreams": [
                            {
                              "dial": ":1200"
                            }
                          ]
                        }
                      ],
                      "match": [
                        {
                          "path": [
                            "/+wck9WA8AWsx/C8k6PibIQ=="
                          ]
                        }
                      ]
                    }
                  ]
                }
              ],
              "match": [
                {
                  "host": [
                    "127.0.0.1",
                    "localhost"
                  ]
                }
              ],
              "terminal": true
            }
          ]
        }
      }
    },
    "tls": {
      "automation": {
        "policies": [
          {
            "issuer": {
              "module": "internal"
            },
            "subjects": [
              "127.0.0.1",
              "localhost"
            ]
          }
        ]
      }
    }
  },
  "logging": {
    "logs": {
      "default": {
        "exclude": [
          "http.log.access.log0",
          "http.log.access.log1"
        ]
      },
      "log0": {
        "include": [
          "http.log.access.log0"
        ],
        "writer": {
          "output": "stdout"
        }
      },
      "log1": {
        "include": [
          "http.log.access.log1"
        ],
        "writer": {
          "output": "stdout"
        }
      }
    }
  }
}
